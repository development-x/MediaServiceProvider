{
    "name": "indominus/media-service-provider",
    "description": "MediaServiceProvider for Silex framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zlatin Hristov",
            "email": "about@z-latko.info",
            "homepage": "https://z-latko.info"
        },
        {
            "name": "DevX Ltd",
            "homepage": "https://devx.info"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "Media\\": "src/Media"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Tests\\": "tests/Media"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "twig/twig": "~1.3",
        "silex/silex": "~2.0",
        "symfony/form": "~3.0",
        "doctrine/orm": "~2.5",
        "symfony/config": "~3.0",
        "symfony/translation": "~3.0",
        "symfony/twig-bridge": "~3.0"
    },
    "suggest": {
        "symfony/translation": "Required by symfony/form",
        "symfony/config": "Required by symfony/translation",
        "symfony/twig-bridge": "Required for render fast forms with symfony/form"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0"
        }
    }
}
